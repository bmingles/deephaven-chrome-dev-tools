{
  "manifest_version": 3,
  "name": "Deephaven Dev - Stats",
  "version": "1.0",
  "description": "Deephaven server stats",
  "devtools_page": "devtools.html",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["http://localhost:10000/*"],
  "icons": {
    "16": "icons/dh-16.png",
    "32": "icons/dh-32.png",
    "48": "icons/dh-48.png",
    "128": "icons/dh-128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
