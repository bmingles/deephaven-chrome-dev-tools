{
  "manifest_version": 3,
  "name": "Deephaven Internal Developer Tools",
  "version": "1.0",
  "description": "Deephaven Internal Developer Tools",
  "permissions": ["activeTab", "contextMenus", "scripting", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://deephaven.atlassian.net/*",
        "https://github.com/deephaven/deephaven.io/pull/*"
      ],
      "css": ["jira.css"]
    }
  ]
}
